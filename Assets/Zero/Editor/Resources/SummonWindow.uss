.unity-box {
    border-radius: 16px;
    background-color: rgba(0, 255, 170, 0.15);
    padding: 12px;
    margin: 12px;
}

.unity-button {
    border-radius: 12px;
    padding: 4px;
}

.unity-base-field__input {
    padding-left: 8px;
    padding-right: 8px;
    border-radius: 8px;
}

.unity-toggle * {
    border-radius: 2px;
}

.box_title {
    font-size: 16px;
    padding: 8px;
}